{
  "name": "fso",
  "version": "2.5.15",
  "description": "fso - FileSystemObject is a objective fs interface like Pathname(ruby)",
  "keywords": [
    "fs",
    "filesystem",
    "pathname"
  ],
  "main": "./dist/lib/fso.js",
  "types": "./src/lib/fso.d.ts",
  "files": [
    "src",
    "dist",
    "web"
  ],
  "scripts": {
    "prepare": "npm run dist",
    "prepublishOnly": "npm run test && npm run lint",
    "dist": "npm run node && npm run web",
    "node": "webpack",
    "web": "webpack --config ./webpack.config.web.js",
    "test": "nyc mocha",
    "lint": "eslint src test",
    "docdist": "cd doc && git clean -fd && git rm -rf * && cd .. && npm run doc && cd doc && touch .nojekyll && git add . && git commit -m doc && git push",
    "doc": "typedoc --mode file --out doc --disableOutputCheck --includeDeclarations --excludeExternals --excludeNotExported src && cd doc && git status && cd .."
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^9.4.7",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-power-assert": "^2.0.0",
    "babel-preset-stage-3": "^6.24.1",
    "browserfs": "^1.4.3",
    "eslint": "^4.18.2",
    "eslint-config-narazaka": "^1.0.0",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "power-assert": "^1.4.4",
    "sinon": "^4.4.2",
    "typedoc": "^0.11.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.11"
  },
  "author": {
    "name": "narazaka",
    "url": "http://narazaka.net/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Narazaka/fso.git"
  }
}
